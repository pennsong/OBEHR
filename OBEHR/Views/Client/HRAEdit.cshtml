@model HRAEditClient

@{
    ViewBag.Title = "编辑";
}

<h2>@ViewBag.Name:@ViewBag.Title</h2>

@using (Html.BeginForm("HRAEditSave", (string)ViewBag.Controller))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        @Html.Hidden("returnUrl", (string)ViewBag.ReturnUrl)

        @Html.EditorForModel()

        <p>
            <input type="submit" value="保存" />
        </p>
    </fieldset>
}

<div>
    <a href="@Url.Content((string)ViewBag.ReturnUrl)">取消</a>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Scripts/CW/jquery.multiselect2side.js")
    <script type="text/javascript">
        $().ready(function () {
            $(".multiselect2side").multiselect2side();
        });
    </script>
}
@section Styles {
    @Styles.Render("~/Content/CW_CSS/jquery.multiselect2side.css")
}